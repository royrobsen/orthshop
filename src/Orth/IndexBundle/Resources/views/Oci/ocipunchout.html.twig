<!DOCTYPE html>
<html>
    <body>
<form action="{{hookurl}}" name="oci_form" method="post" target="_top">  
<input type="hidden" name="~caller" value="CTLG" >  
{% set lindex = 0 %}
{% for result in results %}
{% set lindex = lindex + 1%}
<!-- Item {{lindex}} -->  
<input type="hidden" name="NEW_ITEM-DESCRIPTION[{{lindex}}]" value="{{result.shortName}}"> 
<input type="hidden" name="NEW_ITEM-LONGTEXT[{{lindex}}]" value="{{result.shortName}}">  
<input type="hidden" name="NEW_ITEM-QUANTITY[{{lindex}}]" value="{{result.menge}}">  
<input type="hidden" name="NEW_ITEM-UNIT[{{lindex}}]" value="EA">  
<input type="hidden" name="NEW_ITEM-PRICE[{{lindex}}]" value="{{result.price}}">  
<input type="hidden" name="NEW_ITEM-PRICEUNIT[{{lindex}}]" value="1">  
<input type="hidden" name="NEW_ITEM-CURRENCY[{{lindex}}]" value="EUR">  
<input type="hidden" name="NEW_ITEM-LEADTIME[{{lindex}}]" value="10">  
<input type="hidden" name="NEW_ITEM-VENDORMAT[{{lindex}}]" value="{{result.articleNumber}}">  
<input type="hidden" name="NEW_ITEM-MANUFACTCODE[{{lindex}}]" value="AN1">  
<input type="hidden" name="NEW_ITEM-EXT_QUOTE_ID[{{lindex}}]" value="">  
<input type="hidden" name="NEW_ITEM-EXT_PRODUCT_ID[{{lindex}}]" value="">  
<input type="hidden" name="NEW_ITEM-EXT_SCHEMA_TYPE[{{lindex}}]" value="UNSPSC">  
<input type="hidden" name="NEW_ITEM-SERVICE[{{lindex}}]" value="X" >  
<input type="hidden" name="NEW_ITEM-EXT_CATEGORY_ID[{{lindex}}]" value="{{result.category}}">  
<input type="hidden" name="NEW_ITEM-MATGROUP[{{lindex}}]" value="43100103">  
<input type="hidden" name="NEW_ITEM-PARENT_ID[{{lindex}}]" value="">  
<input type="hidden" name="NEW_ITEM-ITEM_TYPE[{{lindex}}]" value="">  
<input type="hidden" name="NEW_ITEM-CUST_FIELD5[{{lindex}}]" value="/images/product/nw/{{result.image}}">  
<input type="hidden" name="NEW_ITEM-THUMBNAIL_PIC_URL[{{lindex}}]" value="/images/product/nw/{{result.image}}">  

{% endfor %}
<center>  
<input type="submit" value="Transfer shopping cart" id="submit1" name="submit1">  
<br > 
<script type="text/javascript">
document.getElementById("oci_form").submit();
</script>
   </body>
</html>
 