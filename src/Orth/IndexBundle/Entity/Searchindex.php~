<?php

namespace Orth\IndexBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Searchindex
 */
class Searchindex
{
    /**
     * @var string
     */
    private $word;

    /**
     * @var integer
     */
    private $articleRef;

    /**
     * @var integer
     */
    private $id;

    /**
     * @var integer
     */
    private $rank;
    
    /**
     * Set word
     *
     * @param string $word
     * @return Searchindex
     */
    public function setWord($word)
    {
        $this->word = $word;

        return $this;
    }

    /**
     * Get word
     *
     * @return string 
     */
    public function getWord()
    {
        return $this->word;
    }

    /**
     * Set articleRef
     *
     * @param integer $articleRef
     * @return Searchindex
     */
    public function setArticleRef($articleRef)
    {
        $this->articleRef = $articleRef;

        return $this;
    }

    /**
     * Get articleRef
     *
     * @return integer 
     */
    public function getArticleRef()
    {
        return $this->articleRef;
    }
    /**
     * Set rank
     *
     * @param integer rank
     * @return Searchindex
     */
    
    public function setRank($rank)
    {
        $this->rank = $rank;

        return $this;
    }

    /**
     * Get rank
     *
     * @return integer 
     */
    public function getRank()
    {
        return $this->rank;
    }
    
    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }
}
